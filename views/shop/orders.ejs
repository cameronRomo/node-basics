  <%- include("../layouts/head.ejs") %>
  </head>

  <body>
    <%- include("../layouts/navigation.ejs") %>
    <main>
      <% if (orders.length <= 0) { %>
        <h1>None yet</h1>
      <% } else { %>
        <ul>
          <% orders.forEach((order) => { %>
            <li>
              <h1># <%= order.id %></h1>
              <ul>
                <% order.products.forEach((product) => { %>
                  <li><%= product.title %> (<%= product.orderItem.quantity %>)</li>
                <% }) %>
              </ul>
            </li>
          <% }) %>
        </ul>
      <% } %>
    </main>
    <%- include("../layouts/end.ejs") %>
