  <%- include("../layouts/head.ejs") %>
  </head>

  <body>
    <%- include("../layouts/navigation.ejs") %>
    <main>
      <% if (products.length > 0) { %>
        <ul>
          <% products.forEach((p) => {%>
            <li><%= p.productData.title %> (<%= p.qty %>)</li>
          <% }) %>
        </ul>
      <% } else { %>
        <h1>No Products in Cart</h1>
      <% } %>
    </main>
    <%- include("../layouts/end.ejs") %>
